{
  "name": "Pet_shop",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "repository": {
    "type": "git"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "seed": "prisma migrate reset && prisma db seed",
    "build": "yarn && tsc && prisma migrate deploy",
    "predev": "npm install && npm run build",
    "serve": "node -r dotenv/config build/app.js",
    "clean": "rimraf ./build",
    "dev": "concurrently \"tsc --watch\" \"nodemon -q build/app.js\"",
    "start": "node build/app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@prisma/client": "^5.2.0",
    "@types/express": "^4.17.17",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "connect-flash": "^0.1.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "nodemon": "^3.0.2",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "recombee-api-client": "^4.1.4",
    "rimraf": "^5.0.1",
    "sharp": "^0.32.4",
    "typescript": "^5.3.3",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/connect-flash": "^0.0.37",
    "@types/cors": "^2.8.13",
    "@types/express-session": "^1.17.7",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.5",
    "@types/nodemailer": "^6.4.9",
    "@types/passport": "^1.0.12",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-local": "^1.0.35",
    "@types/uuid": "^9.0.2",
    "@types/ws": "^8.5.5",
    "concurrently": "^8.2.0",
    "prisma": "^5.2.0",
    "ts-node": "^10.9.1"
